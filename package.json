{
  "name": "fah-pyon",
  "version": "1.2.0",
  "description": "Parse data in the PyON serialization format",
  "main": "src/load.js",
  "types": "src/index.d.ts",
  "scripts": {
    "preversion": "npm run lint && npm test",
    "version": "npm run build && git add src/grammar",
    "postversion": "git push && git push --tags",
    "test": "ava --tap | tap-nyc",
    "instrument": "nyc npm test",
    "lint": "standard --fix 'src/**/*.js' | snazzy",
    "build": "antlr -o src -Dlanguage=JavaScript -no-listener -visitor grammar/*.g4"
  },
  "keywords": [
    "PyON",
    "data serialization"
  ],
  "author": "Tamara Cook",
  "license": "MIT",
  "repository": "github:tamaracha/node-fah-pyon.git",
  "dependencies": {
    "antlr4": "^4.8.0"
  },
  "devDependencies": {
    "ava": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nyc": "^15.0.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.3",
    "tap-nyc": "^1.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "exclude": "src/grammar/PyONVisitor.js",
    "reporter": [
      "html"
    ],
    "check-coverage": true,
    "lines": 80
  },
  "standard": {
    "ignore": [
      "src/grammar"
    ],
    "parser": "babel-eslint"
  }
}
