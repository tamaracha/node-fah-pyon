{
  "name": "fah-pyon",
  "version": "1.0.2",
  "description": "Parse data in the PyON serialization format",
  "main": "src/load.js",
  "scripts": {
    "preversion": "npm run lint && npm test",
    "version": "npm run build && git add src/grammar",
    "postversion": "git push && git push --tags",
    "test": "riteway src/**/*.spec.js | tap-nyc",
    "instrument": "nyc npm test",
    "lint": "eslint src",
    "build": "antlr -o src -Dlanguage=JavaScript -no-listener -visitor grammar/*.g4"
  },
  "keywords": [
    "PyON",
    "data serialization"
  ],
  "author": "Tamara Cook",
  "license": "MIT",
  "repository": "github:tamaracha/node-fah-pyon.git",
  "dependencies": {
    "antlr4": "^4.7.2"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "nyc": "^14.1.1",
    "prettier-standard": "^14.0.5",
    "react": "^16.9.0",
    "riteway": "^6.1.0",
    "tap-nyc": "^1.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "exclude": "src/grammar/PyONVisitor.js",
    "reporter": [
      "html"
    ],
    "check-coverage": true,
    "lines": 80
  }
}
