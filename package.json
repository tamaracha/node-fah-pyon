{
  "name": "fah-pyon",
  "version": "1.4.1",
  "description": "Parse data in the PyON serialization format",
  "main": "./lib/load.js",
  "type": "commonjs",
  "exports": {
    "import": "./lib/load.mjs",
    "require": "./lib/load.js"
  },
  "types": "./lib/load.d.ts",
  "scripts": {
    "preversion": "npm run lint && npm test",
    "version": "mate -w CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "lint": "standard --fix '{lib,config}/**/*.?(m)js' | snazzy",
    "pretest": "npm run build",
    "test": "ava --tap | tap-nyc",
    "instrument": "nyc npm test",
    "docs": "jsdoc -c ./config/jsdoc.config.json",
    "grammar": "mkdir lib/grammar && nearleyc -o lib/grammar/pyon.js grammar/pyon.ne",
    "prebuild": "del lib/grammar",
    "build": "npm run grammar",
    "prepack": "npm run build"
  },
  "keywords": [
    "PyON",
    "data serialization"
  ],
  "author": "Tamara Cook",
  "license": "MIT",
  "repository": "github:tamaracha/node-fah-pyon.git",
  "files": [
    "lib/**/!(*.spec).{js,mjs,d.ts}"
  ],
  "dependencies": {
    "moo": "^0.5.1",
    "nearley": "^2.19.2"
  },
  "devDependencies": {
    "ava": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "del-cli": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "snazzy": "^8.0.0",
    "standard": "^14.3.3",
    "tap-nyc": "^1.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -c ./config/lint-staged.config.js",
      "pre-push": "npm test"
    }
  },
  "nyc": {
    "exclude": "src/grammar/PyONVisitor.js",
    "reporter": [
      "html"
    ],
    "check-coverage": true,
    "lines": 80
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
